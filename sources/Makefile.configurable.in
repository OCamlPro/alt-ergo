OCAMLVERSION = @OCAMLVERSION@
OCAMLWIN32 = @OCAMLWIN32@
EXE = @EXE@
ENABLEGUI = @ENABLEGUI@
ZARITHLIB=@ZARITHLIB@
NUMLIB=@NUMLIB@
CAMLZIPLIB=@CAMLZIPLIB@
